{
  "checkbox": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=4371%3A10197",
    "checked": {
      "#normal": {
        "background": "@color-theme-control-active-normal",
        "text": "@color-theme-text-primary-normal",
        "border": "none",
        "icon": "@color-theme-inverted-text-primary-normal"
      },
      "#hovered": {
        "background": "@color-theme-control-active-hover",
        "text": "@color-theme-text-primary-normal",
        "border": "none",
        "icon": "@color-theme-inverted-text-primary-normal"
      },
      "#pressed": {
        "background": "@color-theme-control-active-pressed",
        "text": "@color-theme-text-primary-normal",
        "border": "none",
        "icon": "@color-theme-inverted-text-primary-normal"
      },
      "#disabled": {
        "background": "@color-theme-control-active-disabled",
        "text": "@color-theme-text-primary-disabled",
        "border": "none",
        "icon": "@color-theme-control-inactive-hover"
      }
    },
    "unchecked": {
      "#normal": {
        "background": "@color-theme-button-secondary-normal",
        "text": "@color-theme-text-primary-normal",
        "border": "@color-theme-outline-input-normal"
      },
      "#hovered": {
        "background": "@color-theme-button-secondary-hover",
        "text": "@color-theme-text-primary-normal",
        "border": "@color-theme-outline-input-normal"
      },
      "#pressed": {
        "background": "@color-theme-button-secondary-pressed",
        "text": "@color-theme-text-primary-normal",
        "border": "@color-theme-outline-input-normal"
      },
      "#disabled": {
        "background": "@color-theme-button-secondary-disabled",
        "text": "@color-theme-text-primary-disabled",
        "border": "@color-theme-outline-primary-disabled"
      }
    }
  }
}
